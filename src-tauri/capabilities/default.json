{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capabilityforthemainwindow",
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-close",
    "sql:allow-execute",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "dialog:allow-open",
    {
      "identifier": "fs:allow-app-read",
      "allow": [{ "path": "$HOME/**" }]
    }
  ]
}
